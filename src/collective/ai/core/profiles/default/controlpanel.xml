<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.autopublishing"
    purge="False">

  <configlet
      title="collective.ai Control Panel"
      action_id="collective.ai.core"
      appId="collective.ai.core"
      category="Products"
      condition_expr=""
      url_expr="string:${portal_url}/@@collective-ai-settings"
      icon_expr="string:$portal_url/++plone++bootstrap-icons/cpu.svg"
      visible="True"
      i18n:attributes="title">
    <permission>Manage portal</permission>
  </configlet>

</object>
